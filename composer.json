{
  "name": "nystudio107/site-module",
  "description": "An example module for Craft CMS 3 that lets you enhance your websites with a custom site module",
  "version": "1.0.1",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "site",
    "module",
    "site-module"
  ],
  "license": "MIT",
  "homepage": "https://nystudio107.com/",
  "type": "project",
  "support": {
    "email": "info@nystudio107.com",
    "issues": "https://github.com/nystudio107/site-module/issues",
    "source": "https://github.com/nystudio107/site-module",
    "docs": "https://github.com/nystudio107/site-module"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\sitemodule\\": "modules/sitemodule/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
